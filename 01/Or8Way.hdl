/**
 * 8-way Or: 
 * out = (in[0] or in[1] or ... or in[7])
 */

CHIP Or8Way {
    IN in[8];
    OUT out;

    PARTS:
    Or(a=in[0], b=in[1], out=c);
    Or(a=in[2], b=in[3], out=d);
    Or(a=in[4], b=in[5], out=e);
    Or(a=in[6], b=in[7], out=f);

    Or(a=c, b=d, out=cOrd);
    Or(a=e, b=f, out=eOrf);

    Or(a=cOrd, b=eOrf, out=out);
    // Put your code here:
}